{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": ".eslintrc",
            "options": {
                "trailingComma": "none"
            }
        }
    ]
}
