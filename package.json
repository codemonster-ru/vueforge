{
    "name": "@codemonster-ru/vueforge",
    "version": "0.3.5",
    "description": "Open source UI components for Vue.js.",
    "license": "MIT",
    "author": "Kirill Kolesnikov",
    "repository": {
        "type": "git",
        "url": "https://github.com/codemonster-ru/vueforge.git"
    },
    "keywords": [],
    "main": "./dist/index.ts.umd.js",
    "module": "./dist/index.ts.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "style": "./dist/index.css",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.ts.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.ts.umd.js"
            }
        },
        "./dist/*.css": {
            "import": "./dist/*.css",
            "require": "./dist/*.css"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite dev --force --mode development",
        "build": "vite build --mode production"
    },
    "peerDependencies": {
        "vue": "^3.0.0",
        "vue-router": "^4.4.5",
        "@codemonster-ru/vueiconify": "^0.7.0"
    },
    "devDependencies": {
        "pinia": "^2.2.4",
        "@types/node": "^20.14.10",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/eslint-config-typescript": "^13.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "^9.27.0",
        "prettier": "^3.3.2",
        "sass": "^1.77.8",
        "typescript": "^5.5.3",
        "vite": "^6.0.11",
        "vite-plugin-dts": "^4.5.0",
        "vue": "^3.4.31",
        "vue-tsc": "^2.0.21"
    }
}