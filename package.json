{
    "name": "@codemonster-ru/vueforge",
    "version": "0.36.0",
    "description": "Open source UI components for Vue.js.",
    "license": "MIT",
    "author": "Kirill Kolesnikov",
    "repository": {
        "type": "git",
        "url": "https://github.com/codemonster-ru/vueforge.git"
    },
    "keywords": [
        "vue",
        "vue3",
        "ui",
        "components",
        "component-library",
        "ui-kit",
        "design-system",
        "theming",
        "typescript",
        "vite"
    ],
    "main": "./dist/index.ts.umd.js",
    "module": "./dist/index.ts.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "style": "./dist/index.css",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.ts.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.ts.umd.js"
            }
        },
        "./dist/*.css": {
            "import": "./dist/*.css",
            "require": "./dist/*.css"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite dev --force --mode development",
        "build": "vite build --mode production",
        "lint": "node --import tsx ./node_modules/eslint/bin/eslint.js . --ext .ts,.vue --config eslint.config.ts",
        "typecheck": "vue-tsc --noEmit",
        "test": "vitest run"
    },
    "peerDependencies": {
        "@codemonster-ru/vueiconify": "^0.7.0",
        "vue": "^3.0.0",
        "vue-router": "^4.4.5"
    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/test-utils": "^2.4.6",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "^9.27.0",
        "jiti": "^2.4.2",
        "jsdom": "^24.0.0",
        "pinia": "^2.2.4",
        "prettier": "^3.3.2",
        "sass": "^1.77.8",
        "tsx": "^4.20.5",
        "typescript": "^5.5.3",
        "vite": "^6.0.11",
        "vite-plugin-dts": "^4.5.0",
        "vitest": "^4.0.18",
        "vue": "^3.4.31",
        "vue-tsc": "^2.0.21"
    },
    "dependencies": {
        "@codemonster-ru/floater.js": "^1.0.8"
    }
}
